    "type": "magento2-module",
    "license": "OSL-3.0",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Willian Keller",
            "email": "willianlkeller@outlook.com"
        }
    ],
    "support": {
        "issues": "https://github.com/magestat/magento2-split-order/issues/"
